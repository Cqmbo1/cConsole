Usage:


const s = document.createElement("script");
s.src = "https://cdn.jsdelivr.net/gh/Cqmbo1/cConsole@main/1.0.0/cConsole.min.js"
document.body.appendChild(s);
s.onload = function() {
            const cc = new cConsole({ theme: 'dark' }); 
            window.__cconsole = cc;

            cc.register({
                id: 'device',
                name: 'Device',
                render(root, app) {
                    root.innerHTML = '<div class="cc-log">UserAgent: ' + navigator.userAgent + '</div>';
                }
            });

            cc.setTab('network');
        };
